  "local-https": {
    "base-url": "https://localhost:8443",
    "SSLConfiguration": {
      "hasCertificatePassphrase": true,
      "clientCertificate": {
        "format": "P12",
        "path": "../../src/main/resources/server-keystore.p12"
      },
      "verifyHostCertificate": false
    }
  }
}